<bg xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:noNamespaceSchemaLocation='https://raw.githubusercontent.com/RedBrumbler/Quest-BSML-Docs/gh-pages/schema.xsd'>
    <vertical spacing="1" child-expand-width="false" child-expand-height="false" pref-width='70' pref-height='10'>
        <clickable-text active='~is-clickable-top' text='~top-text' font-size="3.5" align='Left' hover-hint='~top-hover-hint' on-click='clicked-top' />
        <text active='~not-is-clickable-top' text='~top-text' font-size="3.5" align='Left' hover-hint='~top-hover-hint'/>
        <clickable-text active='~is-clickable-bottom' text='~bottom-text' font-size="3.5" align='Left' hover-hint='~bottom-hover-hint' on-click='clicked-bottom' />
        <text active='~not-is-clickable-bottom' text='~bottom-text' font-size="3.5" align='Left' hover-hint='~bottom-hover-hint' />
    </vertical>
</bg>