{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "LeaderboardCore",
    "id": "leaderboardcore",
    "version": "0.0.4-rc1",
    "url": "https://github.com/raineio/LeaderboardCore",
    "additionalData": {
      "overrideSoName": "libleaderboardcore.so"
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^3.14.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "modloader",
      "versionRange": "^1.2.3",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.15.23",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.3.1",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "<=1.2.9",
      "additionalData": {}
    },
    {
      "id": "codegen",
      "versionRange": "^0.33.0",
      "additionalData": {}
    },
    {
      "id": "lapiz",
      "versionRange": "^0.1.11",
      "additionalData": {}
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm qmod build --isLibrary true",
        "pwsh ./scripts/createqmod.ps1 LeaderboardCore -clean"
      ]
    }
  }
}